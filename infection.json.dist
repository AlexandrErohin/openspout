{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "src"
        ]
    },
    "timeout": 10,
    "logs": {
        "text": "infections.log",
        "stryker": {
            "report": "/\\d\\.x/"
        }
    },
    "mutators": {
        "@default": true,
        "@number": false,
        "global-ignoreSourceCodeByRegex": [
            "\\\\assert\\(.+\\);"
        ]
    },
    "minMsi": 82,
    "minCoveredMsi": 84
}
