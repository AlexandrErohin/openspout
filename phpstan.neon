includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - phpstan-baseline.neon

parameters:
    level: 6
    paths:
        - src
        - tests
    ignoreErrors:
        -
            message: "#^Method .+Test::dataProvider.+\\(\\) return type has no value type specified in iterable type array\\.$#"
            paths:
                - tests/
